{
  "notes": [
    "This is the dev environment manifest",
    "That's all I have to say"
  ],
  "jenkins": {
    "autodeploy": "yes"
  },
  "versions": {
    "arborist": "quay.io/cdis/arborist:master",
    "aws-es-proxy": "abutaha/aws-es-proxy:0.8",
    "fence": "quay.io/cdis/fence:master",
    "fluentd": "fluent/fluentd-kubernetes-daemonset:v1.2-debian-cloudwatch",
    "indexd": "quay.io/cdis/indexd:master",
    "jenkins": "quay.io/cdis/jenkins:master",
    "jupyterhub": "quay.io/occ_data/jupyterhub:master",
    "peregrine": "quay.io/cdis/peregrine:master",
    "pidgin": "quay.io/cdis/pidgin:master",
    "portal": "quay.io/cdis/data-portal:master",
    "revproxy": "quay.io/cdis/nginx:1.17.6-ctds-1.0.1",
    "sheepdog": "quay.io/cdis/sheepdog:master",
    "spark": "quay.io/cdis/gen3-spark:master",
    "tube": "quay.io/cdis/tube:master",
    "manifestservice": "quay.io/cdis/manifestservice:master",
    "wts": "quay.io/cdis/workspace-token-service:master",
    "guppy": "quay.io/cdis/guppy:master",
    "sower": "quay.io/cdis/sower:master",
    "hatchery": "quay.io/cdis/hatchery:master",
    "metadata": "quay.io/cdis/metadata-service:master",
    "dashboard": "quay.io/cdis/gen3-statics:master",
    "mariner": "quay.io/cdis/mariner-server:master"
  },
  "arborist": {
    "deployment_version": "2"
  },
  "indexd": {
    "arborist": "true"
  },
  "arranger": {
    "project_id": "dev",
    "auth_filter_field": "gen3_resource_path",
    "auth_filter_node_types": [
      "subject"
    ]
  },
  "sower": {
    "name": "pelican-export",
    "container": {
      "name": "job-task",
      "image": "quay.io/cdis/pelican:master",
      "pull_policy": "always",
      "env": []
    },
    "restart_policy": "never"
  },
  "peregrine": {
    "sidecar": "True"
  },
  "global": {
    "environment": "qaplanetv1",
    "hostname": "jenkins-perf.planx-pla.net",
    "revproxy_arn": "arn:aws:acm:us-east-1:707767160287:certificate/c676c81c-9546-4e9a-9a72-725dd3912bc8",
    "dictionary_url": "https://s3.amazonaws.com/dictionary-artifacts/datadictionary/develop/schema.json",
    "portal_app": "dev",
    "sync_from_dbgap": "False",
    "kube_bucket": "kube_bucket.devplanetv1.gen3",
    "logs_bucket": "logs-devplanetv1-gen3",
    "useryaml_s3path": "s3://cdis-gen3-users/qa/user.yaml",
    "netpolicy": "on"
  },
  "jupyterhub": {
    "enabled": "yes",
    "containers": [
      {
        "name": "Bioinfo - Python/R",
        "cpu": 0.5,
        "memory": "256M",
        "image": "quay.io/occ_data/jupyternotebook:feat_workspace-tools"
      }
    ]
  },
  "canary": {
    "default": 0
  },
  "guppy": {
    "indices": [
      {
        "index": "jenkins_subject_alias",
        "type": "subject"
      },
      {
        "index": "jenkins_file_alias",
        "type": "file"
      }
    ],
    "config_index": "jenkins_configs_alias",
    "auth_filter_field": "auth_resource_path"
  },
  "mariner": {
    "storage": {
      "s3": {
        "name": "workflow-engine-garvin",
        "region": "us-east-1"
      }
    },
    "secrets": {
      "awsusercreds": {
        "name": "workflow-bot-g3auto",
        "key": "awsusercreds.json"
      }
    },
    "jobs": {
      "engine": {
        "labels": {
          "app": "mariner-engine"
        },
        "serviceaccount": "mariner-service-account",
        "restart_policy": "never"
      },
      "task": {
        "labels": {
          "app": "mariner-task"
        },
        "restart_policy": "never"
      }
    },
    "containers": {
      "engine": {
        "name": "mariner-engine",
        "image": "quay.io/cdis/mariner-engine:feat_gwas-pilot-races",
        "pull_policy": "always",
        "command": [
          "/bin/sh",
          "/engineDockerrun.sh"
        ],
        "resources": {
          "limits": {
            "memory": "1Gi",
            "cpu": "400m"
          }
        }
      },
      "s3sidecar": {
        "name": "mariner-s3sidecar",
        "image": "quay.io/cdis/mariner-s3sidecar:feat_gwas-pilot-races",
        "pull_policy": "always",
        "command": [
          "/bin/sh",
          "/s3sidecarDockerrun.sh"
        ],
        "lifecycle": {
          "prestop": [
            "/bin/sh",
            "-c",
            "fusermount -u -z /$ENGINE_WORKSPACE"
          ]
        },
        "resources": {
          "limits": {
            "memory": "256Mi",
            "cpu": "100m"
          }
        },
        "securitycontext": {
          "privileged": true
        }
      },
      "gen3fusesidecar": {
        "name": "mariner-gen3fusesidecar",
        "image": "quay.io/cdis/gen3fuse-sidecar:0.2.1",
        "pull_policy": "always",
        "command": [
          "/bin/bash",
          "/marinerRun.sh"
        ],
        "lifecycle": {
          "prestop": [
            "/bin/bash",
            "-c",
            "fusermount -u -z /$COMMONS_DATA"
          ]
        },
        "resources": {
          "limits": {
            "memory": "256Mi",
            "cpu": "100m"
          }
        },
        "securitycontext": {
          "privileged": true
        }
      },
      "task": {
        "name": "mariner-task",
        "image": "alpine",
        "pull_policy": "always",
        "command": [
          "/bin/sh"
        ],
        "resources": {
          "limits": {
            "memory": "512Mi",
            "cpu": "900m"
          }
        }
      }
    }
  },
  "scaling": {
    "arborist": {
      "strategy": "auto"
    },
    "portal": {
      "strategy": "auto",
      "min": 1,
      "max": 2
    },
    "fence": {
      "strategy": "auto"
    },
    "presigned-url-fence": {
      "strategy": "auto"
    },
    "indexd": {
      "strategy": "auto"
    },
    "peregrine": {
      "strategy": "auto"
    },
    "revproxy": {
      "strategy": "auto"
    },
    "sheepdog": {
      "strategy": "auto"
    },
    "guppy": {
      "strategy": "auto"
    }
  }
}
